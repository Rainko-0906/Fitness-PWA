# 测试环境配置
NODE_ENV=test
PORT=3002

# 测试数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_NAME=fitness_db_test
DB_USER=postgres
DB_PASSWORD=postgres

# 测试 JWT 密钥
JWT_SECRET=test-secret-key
JWT_EXPIRES_IN=1h

# 测试速率限制
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=1000

# 测试缓存配置
CACHE_TTL=60

# 测试日志级别
LOG_LEVEL=error

# 测试 Firebase 配置
