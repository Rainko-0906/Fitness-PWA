FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nTestKey\n-----END PRIVATE KEY-----"

# 测试 Supabase 配置
SUPABASE_URL=https://fitness-pwa-test.supabase.co
SUPABASE_ANON_KEY=test-supabase-key

# 测试 OpenAI 配置
OPENAI_API_KEY=test-openai-key
